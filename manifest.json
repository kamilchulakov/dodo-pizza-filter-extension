{
  "manifest_version": 1,
  "name": "Pizza Helper",
  "version": "1.0",

  "description": "Adds a filter by ingredients to dodopizza.ru .",

  "icons": {
    "bird": "dodo.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.dodopizza.ru/*"],
      "js": ["pizza_helper.js"]
    }
  ]

}